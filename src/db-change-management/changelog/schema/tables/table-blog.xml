<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    

    <changeSet author="on-boarding-seed" context="standard" id="create-table-blog">
        <createTable tableName="blog">
            <column autoIncrement="true" computed="false" name="id" type="int">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_blog"/>
            </column>
            <column name="post" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
